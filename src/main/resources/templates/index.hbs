{{#partial "content"}}

    <h1>Startup Event list!</h1>

    {{#if events}}
        {{#each events}}
            <div class="col-md-6">
                <p><a href="/event/{{id}}">{{name}}</a></p>

            </div>
        {{/each}}
    {{else}}
        <p>No Events Available!</p>
    {{/if}}

    <footer class="footer">
        <div class="container">
            <!--create route to delete all events -->
            <!--<p class="text-muted"><a href="/event/delete">Delete All Events!</a></p>-->
        </div>
    </footer>
{{/partial}}

{{> layout.hbs}}